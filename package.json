      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node"
  },
  "dependencies": {
    "next": "^13.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.132.2"
